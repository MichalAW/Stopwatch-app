{
    "name": "19.6.Stopwatch-app",
    "version": "1.0.0",
    "description": "Stopwatch app",
    "main": "script.js",
    "scripts": {
        "test": "npm-run-all",
        "test:html": "globstar nu-html-checker *.html",
        "test:echo": "echo \"Error: no test specified\" && exit 1",
        "start:project": "npm install && npm-run-all init:*",
        "start:babel": "babel script.js --watch --out-file script-compiled.js",
        "build": "npm-run-all build:* test",
        "build:clean": "rimraf css/style.css",
        "build:sass": "node-sass --output-style compact -o css sass",
        "build:autoprefixer": "autoprefixer-cli css/style.css",
        "build-dev:sass": "node-sass --output-style expanded --source-map true -o css sass",
        "watch": "npm-run-all build -p watch:*",
        "watch:sassprefixer": "node-sass --output-style expanded --source-map true -o css sass --watch && onchange -d 100 css/style.css -- autoprefixer-cli -o css/style.prefixed.css css/style.css",
        "watch:browsersync": "browser-sync start --server --files css/*.css *.html"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/MichalAW/19.6.Stopwatch-app.git"
    },
    "keywords": [],
    "author": "MichalAW michal.wanta@gmail.com",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/MichalAW/19.6.Stopwatch-app/issues"
    },
    "homepage": "https://github.com/MichalAW/19.6.Stopwatch-app#readme",
    "devDependencies": {
        "autoprefixer-cli": "^1.0.0",
        "babel": "^6.23.0",
        "babel-cli": "^6.26.0",
        "babel-preset-latest": "^6.24.1",
        "browser-sync": "^2.26.3",
        "globstar": "^1.0.0",
        "http-server": "^0.11.1",
        "node-sass": "^4.11.0",
        "onchange": "^5.2.0"
    },
    "dependencies": {
        "colors": "^1.3.3",
        "formidable": "^1.2.1",
        "npm-run-all": "^4.1.5",
        "nu-html-checker": "^0.1.0",
        "sass": "^1.16.0",
        "save-dev": "^2.0.0",
        "stat-mode": "^0.3.0"
    }
}
